xml.AddElement(HistoryLog.GetSerializedLog());
}
catch(error){
xml.AddElement("<RTL_ERROR>There was an error attaching the history log to this XML, see Controller_GetXmlForDirtyData</RTL_ERROR>");
}
var _af=this.Activities.GetRootActivity();
var _b0=new XmlElement("RS");
var _b1;
if(_af.GetPrimaryObjective().ProgressStatus){
_b1=_af.GetPrimaryObjective().SatisfiedStatus?"passed":"failed";
}else{
_b1="unknown";
}
_b0.AddAttribute("SS",_b1);
var _b2;
if(_af.AttemptProgressStatus){
_b2=_af.AttemptCompletionStatus?"complete":"incomplete";
}else{
_b2="unknown";
}
_b0.AddAttribute("CS",_b2);
_b0.AddAttribute("MS",_a8.ConvertBoolean(_af.GetPrimaryObjective().MeasureStatus));
_b0.AddAttribute("NM",_af.GetPrimaryObjective().NormalizedMeasure);
_b0.AddAttribute("ED",_a8.ConvertTimeSpan(_af.ActivityExperiencedDurationTracked));
