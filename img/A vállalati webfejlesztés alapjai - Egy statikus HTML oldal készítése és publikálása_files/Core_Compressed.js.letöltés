ret+="No Child Activity Meets ";
}
}
}
}
}
if(this.ConditionCombination==RULE_CONDITION_COMBINATION_ANY){
ret+=" Any Condition ";
}else{
ret+=" All Conditions ";
}
ret+=" THEN "+this.Action;
if(this.Conditions.length>1){
ret+=". Conditions: ";
