case SCORM_RUNTIME_NAV_REQUEST_SUSPENDALL:
return this.PossibleNavigationRequests[POSSIBLE_NAVIGATION_REQUEST_INDEX_SUSPEND_ALL];
case SCORM_RUNTIME_NAV_REQUEST_NONE:
return null;
default:
