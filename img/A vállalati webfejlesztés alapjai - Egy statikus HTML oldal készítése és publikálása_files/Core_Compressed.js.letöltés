xml.AddAttribute("SMi",this.ScoreMin);
}
xml.AddAttribute("PrMS",_449.ConvertBoolean(this.ProgressMeasureStatus));
if(this.ProgressMeasure!==null){
xml.AddAttribute("PM",this.ProgressMeasure);
}
return xml.toString();
}
