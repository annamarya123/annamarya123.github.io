var _78=this.Sequencer.CurrentActivity;
var _79=this.Api.RunTimeData.SessionTime;
var _7a=this.Api.RunTimeData.TotalTime;
var _7b=this.Api.RunTimeData.TotalTimeTracked;
var _7c=this.Api.RunTimeData.Entry;
var _7d=this.Api.RunTimeData.CompletionStatus;
var _7e=this.Api.RunTimeData.CompletionStatusChangedDuringRuntime;
var _7f=this.Api.RunTimeData.SuccessStatus;
var _80=this.Api.RunTimeData.SuccessStatusChangedDuringRuntime;
var _81=this.Api.RunTimeData.NavRequest;
