xml.AddAttribute("S",_374.ConvertBoolean(this.Suspended));
xml.AddAttribute("I",_374.ConvertBoolean(this.Included));
xml.AddAttribute("O",this.Ordinal);
xml.AddAttribute("SC",_374.ConvertBoolean(this.SelectedChildren));
xml.AddAttribute("RC",_374.ConvertBoolean(this.RandomizedChildren));
