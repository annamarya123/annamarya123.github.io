this.WriteHistoryLog("",{ev:"Unload"});
if(typeof GetCurrentRegistration!="undefined"){
localStorage.registrationJson=GetCurrentRegistration();
localStorage.runtimeXml=Control.GetXmlForDirtyData();
}
