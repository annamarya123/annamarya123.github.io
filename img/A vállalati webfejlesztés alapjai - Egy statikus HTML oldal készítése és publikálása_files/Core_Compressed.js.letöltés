xml.AddAttribute("SS",_449.ConvertBoolean(this.SatisfiedStatus));
xml.AddAttribute("MS",_449.ConvertBoolean(this.MeasureStatus));
xml.AddAttribute("NM",this.NormalizedMeasure);
xml.AddAttribute("CS",_449.ConvertBoolean(this.CompletionStatus));
xml.AddAttribute("CSV",_449.ConvertBoolean(this.CompletionStatusValue));
