this.LookAheadSuccessStatus=this.SuccessStatus;
this.SessionTime="";
this.NavRequest=SCORM_RUNTIME_NAV_REQUEST_NONE;
this.CompletionStatusChangedDuringRuntime=false;
this.SuccessStatusChangedDuringRuntime=false;
this.SetDirtyData();
}
