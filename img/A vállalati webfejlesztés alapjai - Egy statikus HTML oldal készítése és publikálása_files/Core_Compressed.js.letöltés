xml.AddAttribute("AtEDT",_374.ConvertTimeSpan(this.AttemptExperiencedDurationTracked));
xml.AddAttribute("AEDR",_374.ConvertTimeSpan(this.ActivityExperiencedDurationReported));
xml.AddAttribute("AtEDR",_374.ConvertTimeSpan(this.AttemptExperiencedDurationReported));
}
