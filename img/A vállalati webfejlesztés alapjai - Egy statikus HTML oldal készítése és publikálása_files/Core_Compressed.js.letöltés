if((_bf===false||allUIHidden)&&this.PossibleNavigationRequests[POSSIBLE_NAVIGATION_REQUEST_INDEX_EXIT_ALL].WillSucceed===true){
return new NavigationRequest(NAVIGATION_REQUEST_EXIT_ALL,null,"");
}
}else{
if(_bc=="suspend_all"){
if((_bd===false||allUIHidden)&&this.PossibleNavigationRequests[POSSIBLE_NAVIGATION_REQUEST_INDEX_SUSPEND_ALL].WillSucceed===true){
return new NavigationRequest(NAVIGATION_REQUEST_SUSPEND_ALL,null,"");
}
}
}
}
if(_be===false&&this.PossibleNavigationRequests[POSSIBLE_NAVIGATION_REQUEST_INDEX_ABANDON_ALL].WillSucceed===true){
return new NavigationRequest(NAVIGATION_REQUEST_ABANDON_ALL,null,"");
}else{
